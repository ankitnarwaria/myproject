<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>myproject</display-name>
  <welcome-file-list>
    <welcome-file>form1.html</welcome-file>
  </welcome-file-list>
   <servlet>
    <servlet-name>ar</servlet-name>
    <servlet-class>myproject.AadharRegistration</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>sr</servlet-name>
    <servlet-class>myproject.StudentRegistration</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>cr</servlet-name>
    <servlet-class>myproject.CourseRegistration</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ar</servlet-name>
    <url-pattern>/reg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>sr</servlet-name>
    <url-pattern>/stdreg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>cr</servlet-name>
    <url-pattern>/crsreg</url-pattern>
  </servlet-mapping>
</web-app>